# Environment variables for deployment
# Copy this to .env and fill in your values

# Your wallet private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Alchemy or Infura API key for Sepolia
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key-here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Education fund address (where yield will be sent)
EDUCATION_FUND_ADDRESS=your_education_fund_wallet_address_here
